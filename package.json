    "@types/expo__vector-icons": "^6.2.2",
    "@types/jasmine": "^2.8.8",
    "@types/react": "^16.4.7",
    "@types/react-native": "^0.56.4",
    "awesome-typescript-loader": "^5.2.0",
    "babel-jest": "22.2.2",
    "babel-preset-jest": "^23.2.0",
    "babel-preset-react-es2015": "^1.3.0",
    "enzyme": "^3.3.0",
    "jest": "22.2.2",
    "jest-expo": "25.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.4.2",
    "react-intl": "^2.4.0",
    "react-native-scripts": "^1.14.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-navigation": "^2.9.1",
    "react-test-renderer": "16.3.1",
    "storybook-addon-intl": "^2.3.0",
    "ts-jest": "^23.0.1",
    "tslib": "^1.9.3",
    "typescript": "2.9.2",
    "watchman": "^1.0.0",
    "@blueeast/tslint-config-blueeast": "0.5.6"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "web": "react-scripts start",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'src/**/*.tsx'",
    "test": "jest --coverage",
    "jest": "jest -u",
    "storybook": "storybook start -p 7007",
    "webpack": "webpack"
  },
  "babel": {
    "presets": [
      "expo"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@expo/vector-icons)/"
    ],
    "testRegex": "(/src/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileMock.js",
      "\\.(css|less)$": "<rootDir>/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.56",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.1",
    "@types/react-dom": "^16.0.6",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-client": "^2.3.7",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expo": "^27.0.1",
    "formik": "^1.0.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "jest-react-native": "^18.0.0",
    "jsdom": "^11.12.0",
    "react-apollo": "^2.1.9",
    "react-native": "^0.55.4",
    "react-native-vector-icons": "^5.0.0",
    "react-native-web": "^0.8.9",
    "react-router": "^4.3.1",
    "react-router-native": "^4.3.0",
    "source-map-loader": "^0.2.3",
    "subscriptions-transport-ws": "^0.9.14",
    "typescript": "^2.9.2",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "yup": "0.24.1"
  }
}
